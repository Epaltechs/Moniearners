{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["next", "next/core-web-vitals", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
